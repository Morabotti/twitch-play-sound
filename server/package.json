{
  "name": "twitch-play-sound-server",
  "version": "1.0.0",
  "description": "twitch-play-sound-server",
  "author": "Jesse BÃ¥tman (http://jessebatman.fi)",
  "main": "src/server.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Morabotti/twitch-play-sound.git"
  },
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "dev": "nodemon",
    "build": "tsc",
    "build:start": "tsc && node dist/index.js",
    "production": "cd ../client & npm run build && cd ../server & npm run build:start",
    "server": "node dist/index.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "eslint \"src/**/*.{ts,tsx}\" --fix"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^7.0.0",
    "eslint": "5.16.0",
    "eslint-config-jubic-typescript": "1.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "express": "^4.16.4",
    "jsonfile": "^5.0.0",
    "moment": "^2.24.0",
    "multer": "^1.4.1",
    "socket.io": "^2.2.0",
    "tmi.js": "^1.4.5",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3333",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/jsonfile": "^5.0.0",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.3.26",
    "@types/multer": "^1.3.7",
    "@types/node": "^11.11.3",
    "@types/socket.io": "^2.1.2",
    "@types/tmi.js": "^1.4.0",
    "@types/uuid": "^3.4.5",
    "csgo-gsi-types": "^1.0.5",
    "nodemon": "^1.18.9",
    "ts-essentials": "^2.0.7"
  }
}
